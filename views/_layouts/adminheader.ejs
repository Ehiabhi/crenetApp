                    <a class="nav-link" href="/admin/products">Products</a>
                </li>
            </ul>
        </div>
    </nav>

    <main role="main" class="container">
        <%- messages("messages", locals) %>

        <% if (errors) { %>
        <% errors.forEach(function(error) { %>
        <div class="alert alert-danger">
            <%= error.msg %>
        </div>
        <% }); %>
        <% } %>